{
  "name": "@ai-chat/core",
  "version": "1.0.0",
  "description": "Core AI service wrappers, configuration, and utilities shared across AI Chat apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "echo 'No tests specified'",
    "test:integration:prepare": "npm run -s build && tsc -p tsconfig.tests.json",
    "test:grok": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-grok.js",
    "test:openai": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-openai.js",
    "test:mistral": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-mistral.js",
    "test:gemini": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-gemini.js",
    "test:anthropic": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-anthropic.js",
    "test:deepseek": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-deepseek.js",
    "test:qwen": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-qwen.js",
    "test:kimi": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-kimi.js",
    "test:zai": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-zai.js",
    "test:cohere": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-cohere.js",
    "test:llama": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-llama.js",
    "test:perplexity": "npm run -s test:integration:prepare && node dist-tests/tests/integration-ok-perplexity.js",
    "test:all": "npm run -s test:integration:prepare ; node dist-tests/tests/integration-ok-openai.js ; node dist-tests/tests/integration-ok-mistral.js ; node dist-tests/tests/integration-ok-grok.js ; node dist-tests/tests/integration-ok-gemini.js ; node dist-tests/tests/integration-ok-anthropic.js ; node dist-tests/tests/integration-ok-deepseek.js ; node dist-tests/tests/integration-ok-qwen.js ; node dist-tests/tests/integration-ok-kimi.js ; node dist-tests/tests/integration-ok-zai.js ; node dist-tests/tests/integration-ok-cohere.js ; node dist-tests/tests/integration-ok-llama.js ; node dist-tests/tests/integration-ok-perplexity.js",
    "lint": "eslint . --ext js,ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-chat/ai-configs": "file:../ai-configs",
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.24.0",
    "@mistralai/mistralai": "^1.5.2",
    "dotenv": "^16.4.7",
    "node-fetch": "^3.3.2",
    "openai": "^4.89.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "eslint": "^8.56.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "ai",
    "chat",
    "realtime",
    "websocket"
  ],
  "author": "AI Chat Team",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  }
}
