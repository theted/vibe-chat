{
  "name": "ai-chat",
  "version": "1.0.0",
  "description": "Vibe chat application with multiple AI personalities and CLI tools",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "./start.sh",
    "cli": "pnpm --filter @ai-chat/cli run start",
    "build:configs": "turbo run build --filter=@ai-chat/ai-configs",
    "build:core": "turbo run build --filter=@ai-chat/core",
    "build:cli": "turbo run build --filter=@ai-chat/cli",
    "build:server": "turbo run build --filter=@ai-chat/server",
    "build:client": "turbo run build --filter=@ai-chat/client",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:configs": "turbo run test --filter=@ai-chat/ai-configs",
    "test:core": "turbo run test --filter=@ai-chat/core",
    "test:client": "turbo run test --filter=@ai-chat/client",
    "test:cli": "turbo run test --filter=@ai-chat/cli",
    "test:server": "turbo run test --filter=@ai-chat/server",
    "test:e2e": "pnpm exec codeceptjs run --steps",
    "test:grok": "pnpm --filter @ai-chat/core run test:grok",
    "test:openai": "pnpm --filter @ai-chat/core run test:openai",
    "test:mistral": "pnpm --filter @ai-chat/core run test:mistral",
    "test:gemini": "pnpm --filter @ai-chat/core run test:gemini",
    "test:anthropic": "pnpm --filter @ai-chat/core run test:anthropic",
    "test:deepseek": "pnpm --filter @ai-chat/core run test:deepseek",
    "test:qwen": "pnpm --filter @ai-chat/core run test:qwen",
    "test:kimi": "pnpm --filter @ai-chat/core run test:kimi",
    "test:zai": "pnpm --filter @ai-chat/core run test:zai",
    "test:cohere": "pnpm --filter @ai-chat/core run test:cohere",
    "test:llama": "pnpm --filter @ai-chat/core run test:llama",
    "test:nvidia": "pnpm --filter @ai-chat/core run test:nvidia",
    "test:xiaomi": "pnpm --filter @ai-chat/core run test:xiaomi",
    "test:minimax": "pnpm --filter @ai-chat/core run test:minimax",
    "test:baidu": "pnpm --filter @ai-chat/core run test:baidu",
    "test:bytedance": "pnpm --filter @ai-chat/core run test:bytedance",
    "test:huggingface": "pnpm --filter @ai-chat/core run test:huggingface",
    "test:perplexity": "pnpm --filter @ai-chat/core run test:perplexity",
    "test:all": "pnpm --filter @ai-chat/core run test:all",
    "lint": "turbo run lint",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:client": "turbo run lint --filter=@ai-chat/client",
    "lint:server": "turbo run lint --filter=@ai-chat/server",
    "lint:core": "turbo run lint --filter=@ai-chat/core",
    "docker:dev": "docker compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker compose up --build",
    "docker:debug": "docker compose -f docker-compose.debug.yml up --build",
    "docker:clean": "docker compose down --volumes && docker system prune -f"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^13.24.0",
    "prettier": "^3.3.3",
    "turbo": "^2.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "ai",
    "chat",
    "realtime",
    "websocket",
    "react",
    "nodejs",
    "openai",
    "anthropic"
  ],
  "author": "AI Chat Team",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
