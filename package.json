{
  "name": "ai-chat",
  "version": "1.0.0",
  "description": "A project that allows different AI systems to communicate with each other without external control",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "start": "tsx index.ts",
    "dev": "tsx watch index.ts",
    "play": "tsx play.ts",
    "test": "echo \"Specify a test script. Try: npm run test:grok\"",
    "test:grok": "tsx tests/integration-ok-grok.js",
    "test:openai": "tsx tests/integration-ok-openai.js",
    "test:mistral": "tsx tests/integration-ok-mistral.js",
    "test:gemini": "tsx tests/integration-ok-gemini.js",
    "test:anthropic": "tsx tests/integration-ok-anthropic.js",
    "test:deepseek": "tsx tests/integration-ok-deepseek.js",
    "test:qwen": "tsx tests/integration-ok-qwen.js",
    "test:kimi": "tsx tests/integration-ok-kimi.js",
    "test:zai": "tsx tests/integration-ok-zai.js",
    "test:cohere": "tsx tests/integration-ok-cohere.js",
    "test:all": "npm run -s test:openai && npm run -s test:mistral && npm run -s test:grok && npm run -s test:gemini && npm run -s test:anthropic && npm run -s test:deepseek && npm run -s test:qwen && npm run -s test:kimi && npm run -s test:zai && npm run -s test:cohere"
  },
  "keywords": [
    "ai",
    "chat",
    "communication",
    "openai",
    "anthropic"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-chat/core": "file:./ai-chat-realtime/packages/ai-chat-core",
    "dotenv": "^16.4.7",
    "node-fetch": "^3.3.2",
    "redis": "^4.6.13"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
