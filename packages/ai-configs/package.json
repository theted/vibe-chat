{
  "name": "@ai-chat/ai-configs",
  "version": "1.0.0",
  "description": "Centralized AI model configurations, emojis, and metadata for AI Chat",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf dist",
    "prepare": "bun run build",
    "prepublishOnly": "bun run clean && bun run build",
    "test": "bunx tsx --test tests/**/*.test.ts",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "ai",
    "config",
    "models",
    "emoji"
  ],
  "author": "AI Chat Team",
  "license": "MIT",
  "engines": {
    "bun": ">=1.2.0"
  }
}
