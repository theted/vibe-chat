{
  "name": "@ai-chat/core",
  "version": "1.0.0",
  "description": "Core AI service wrappers, configuration, and utilities shared across AI Chat apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm --prefix ../ai-configs run build",
    "build": "tsc --emitDeclarationOnly --outDir dist && swc src -d dist --strip-leading-paths && bun ../../scripts/resolve-path-aliases.mjs .",
    "build:watch": "swc src -d dist --watch & tsc --noEmit --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run -s test:integration:mocked",
    "test:integration:prepare": "npm run -s build && tsc -p tsconfig.tests.json && bun ../../scripts/resolve-test-aliases.mjs .",
    "test:integration:mocked": "npm run -s test:integration:prepare && bun --test dist-tests/tests/**/*.test.js",
    "test:grok": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-grok.js",
    "test:openai": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-openai.js",
    "test:mistral": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-mistral.js",
    "test:gemini": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-gemini-comprehensive.js",
    "test:anthropic": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-anthropic.js",
    "test:deepseek": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-deepseek.js",
    "test:qwen": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-qwen.js",
    "test:kimi": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-kimi.js",
    "test:zai": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-zai.js",
    "test:cohere": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-cohere.js",
    "test:llama": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-llama.js",
    "test:nvidia": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-nvidia.js",
    "test:xiaomi": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-xiaomi.js",
    "test:minimax": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-minimax.js",
    "test:baidu": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-baidu.js",
    "test:bytedance": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-bytedance.js",
    "test:huggingface": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-huggingface.js",
    "test:perplexity": "npm run -s test:integration:prepare && bun dist-tests/tests/integration-ok-perplexity.js",
    "test:all": "npm run -s test:integration:prepare ; bun dist-tests/tests/integration-ok-openai.js ; bun dist-tests/tests/integration-ok-mistral.js ; bun dist-tests/tests/integration-ok-grok.js ; bun dist-tests/tests/integration-ok-gemini.js ; bun dist-tests/tests/integration-ok-anthropic.js ; bun dist-tests/tests/integration-ok-deepseek.js ; bun dist-tests/tests/integration-ok-qwen.js ; bun dist-tests/tests/integration-ok-kimi.js ; bun dist-tests/tests/integration-ok-zai.js ; bun dist-tests/tests/integration-ok-cohere.js ; bun dist-tests/tests/integration-ok-llama.js ; bun dist-tests/tests/integration-ok-nvidia.js ; bun dist-tests/tests/integration-ok-xiaomi.js ; bun dist-tests/tests/integration-ok-minimax.js ; bun dist-tests/tests/integration-ok-baidu.js ; bun dist-tests/tests/integration-ok-bytedance.js ; bun dist-tests/tests/integration-ok-huggingface.js ; bun dist-tests/tests/integration-ok-perplexity.js",
    "lint": "eslint . --ext js,ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-chat/ai-configs": "file:../ai-configs",
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.24.0",
    "@mistralai/mistralai": "^1.5.2",
    "dotenv": "^16.4.7",
    "node-fetch": "^3.3.2",
    "openai": "^4.89.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.0",
    "@swc/core": "^1.4.0",
    "@types/node": "^24.10.1",
    "eslint": "^8.56.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "ai",
    "chat",
    "realtime",
    "websocket"
  ],
  "author": "AI Chat Team",
  "license": "MIT",
  "engines": {
    "bun": ">=1.1.0"
  }
}
