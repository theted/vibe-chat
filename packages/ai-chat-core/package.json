{
  "name": "@ai-chat/core",
  "version": "1.0.0",
  "description": "Core AI service wrappers, configuration, and utilities shared across AI Chat apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "bun --cwd ../ai-configs run build",
    "build": "tsc --emitDeclarationOnly --outDir dist && swc src -d dist --strip-leading-paths && bun ../../scripts/resolve-path-aliases.mjs .",
    "build:watch": "swc src -d dist --watch & tsc --noEmit --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "test": "bun run build && bun test tests/**/*.test.ts",
    "test:grok": "bun run build && bun tests/integration-ok-grok.ts",
    "test:openai": "bun run build && bun tests/integration-ok-openai.ts",
    "test:mistral": "bun run build && bun tests/integration-ok-mistral.ts",
    "test:gemini": "bun run build && bun tests/integration-gemini-comprehensive.ts",
    "test:anthropic": "bun run build && bun tests/integration-ok-anthropic.ts",
    "test:deepseek": "bun run build && bun tests/integration-ok-deepseek.ts",
    "test:qwen": "bun run build && bun tests/integration-ok-qwen.ts",
    "test:kimi": "bun run build && bun tests/integration-ok-kimi.ts",
    "test:zai": "bun run build && bun tests/integration-ok-zai.ts",
    "test:cohere": "bun run build && bun tests/integration-ok-cohere.ts",
    "test:llama": "bun run build && bun tests/integration-ok-llama.ts",
    "test:nvidia": "bun run build && bun tests/integration-ok-nvidia.ts",
    "test:xiaomi": "bun run build && bun tests/integration-ok-xiaomi.ts",
    "test:minimax": "bun run build && bun tests/integration-ok-minimax.ts",
    "test:baidu": "bun run build && bun tests/integration-ok-baidu.ts",
    "test:bytedance": "bun run build && bun tests/integration-ok-bytedance.ts",
    "test:huggingface": "bun run build && bun tests/integration-ok-huggingface.ts",
    "test:perplexity": "bun run build && bun tests/integration-ok-perplexity.ts",
    "test:orchestrator-timing": "bun run build && bun test tests/integration-orchestrator-timing.test.ts",
    "test:all": "bun run build ; bun tests/integration-ok-openai.ts ; bun tests/integration-ok-mistral.ts ; bun tests/integration-ok-grok.ts ; bun tests/integration-gemini-comprehensive.ts ; bun tests/integration-ok-anthropic.ts ; bun tests/integration-ok-deepseek.ts ; bun tests/integration-ok-qwen.ts ; bun tests/integration-ok-kimi.ts ; bun tests/integration-ok-zai.ts ; bun tests/integration-ok-cohere.ts ; bun tests/integration-ok-llama.ts ; bun tests/integration-ok-nvidia.ts ; bun tests/integration-ok-xiaomi.ts ; bun tests/integration-ok-minimax.ts ; bun tests/integration-ok-baidu.ts ; bun tests/integration-ok-bytedance.ts ; bun tests/integration-ok-huggingface.ts ; bun tests/integration-ok-perplexity.ts",
    "lint": "eslint . --ext js,ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-chat/ai-configs": "file:../ai-configs",
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.24.0",
    "@mistralai/mistralai": "^1.5.2",
    "dotenv": "^16.4.7",
    "openai": "^4.89.0"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^15.1.0",
    "@swc/cli": "^0.3.0",
    "@swc/core": "^1.4.0",
    "@types/node": "^24.10.1",
    "@types/sinonjs__fake-timers": "^15.0.1",
    "eslint": "^8.56.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "ai",
    "chat",
    "realtime",
    "websocket"
  ],
  "author": "AI Chat Team",
  "license": "MIT",
  "engines": {
    "bun": ">=1.2.0"
  }
}
